type Query{
    accessToken(tokenString: String!): Token
}

type Mutation{
    authenticate(credential: Credential!):  TokenPair
    createAccount(credential: Credential!): TokenPair
}

enum TokenType{
    ACCESS
    REFRESH
    ID
}

type Token{
    type: TokenType!
    value: String!
}

input Credential{
    id: String
    email: String!
    password: String!
}

type TokenPair{
    accessToken: Token!
    refreshToken: Token!
}