type Query{
    accessToken(tokenString: String!): Token!
}

type Mutation{
    authenticate(credential: Credential!):  Token!
    createAccount(credential: Credential!): Token!
}

enum TokenType{
    ACCESS
    REFRESH
    ID
}

type Token{
    type: TokenType!
    value: String!
}

input Credential{
    id: String
    email: String!
    password: String!
}